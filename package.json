{
  "name": "@whittenio/nodejs-starter-kit",
  "version": "1.0.0",
  "author": "Rex Whitten <rex@whitten.io>",
  "license": "MIT",
  "description": "NodeJS Starter kit.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitten-io/nodejs-starter-kit.git"
  },
   "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --config ./jest.config.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "start": "node dist/server.js",
    "start:dev" : "nodemon --config nodemon.json ./src/main.ts",
    "start:debug": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "dev:server": "ts-node src/main.ts",
    "docs": "typedoc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "keywords": [
    "typescript",
    "nodemon",
    "jest",
    "eslint"
  ],
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typedoc": "^0.23.23",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "eslint-plugin-json": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0"
  }
}
